<div class="page-header">
  <%= link_to stories_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_story_path(@story), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show story</h1>
</div>

<dl class="dl-horizontal">
  <dt>Number:</dt>
  <dd><%= @story.number %></dd>

  <dt>Title:</dt>
  <dd><%= @story.title %></dd>

  <dt>Email:</dt>
  <dd><%= @story.email %></dd>

</dl>
